import java.util.*;


public class Main {

    public static void main(String args[]) {

		Scanner in = new Scanner (System.in);
		int k = in.nextInt();
		int digit = in.nextInt();
		int count = 0;
		
		while (k>0){
			int d = k % 10;

			if(d  == digit){
				count++;
			} 
			k = k / 10;
		}

		System.out.println(count);

	}
}
